#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/mnt/c/invoice/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/mnt/c/invoice/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.11/node_modules/webpack/node_modules:/mnt/c/invoice/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.11/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
fi
