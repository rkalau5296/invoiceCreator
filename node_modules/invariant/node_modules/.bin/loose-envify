#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/mnt/c/invoice/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/.pnpm/invariant@2.2.4/node_modules/invariant/node_modules:/mnt/c/invoice/node_modules/.pnpm/invariant@2.2.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
fi
