#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/frontend/node_modules/.pnpm/regjsparser@0.6.9/node_modules/regjsparser/bin/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/regjsparser@0.6.9/node_modules/regjsparser/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/regjsparser@0.6.9/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/node_modules:/mnt/c/invoice/frontend/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/regexpu-core@4.7.1/node_modules/regexpu-core/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/regexpu-core@4.7.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../regjsparser@0.6.9/node_modules/regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../../../../../regjsparser@0.6.9/node_modules/regjsparser/bin/parser" "$@"
fi
