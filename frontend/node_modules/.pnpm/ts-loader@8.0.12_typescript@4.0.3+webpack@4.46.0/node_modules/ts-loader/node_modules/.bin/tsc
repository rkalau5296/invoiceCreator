#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/frontend/node_modules/.pnpm/typescript@4.0.3/node_modules/typescript/bin/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/typescript@4.0.3/node_modules/typescript/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/typescript@4.0.3/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/node_modules:/mnt/c/invoice/frontend/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/ts-loader@8.0.12_typescript@4.0.3+webpack@4.46.0/node_modules/ts-loader/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/ts-loader@8.0.12_typescript@4.0.3+webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.0.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.0.3/node_modules/typescript/bin/tsc" "$@"
fi
