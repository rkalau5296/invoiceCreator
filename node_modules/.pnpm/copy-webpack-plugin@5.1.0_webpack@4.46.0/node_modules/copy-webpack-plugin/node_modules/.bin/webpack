#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.11/node_modules/webpack/bin/node_modules:/mnt/c/invoice/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.11/node_modules/webpack/node_modules:/mnt/c/invoice/node_modules/.pnpm/webpack@4.46.0_webpack-cli@3.3.11/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/.pnpm/copy-webpack-plugin@5.1.0_webpack@4.46.0/node_modules/copy-webpack-plugin/node_modules:/mnt/c/invoice/node_modules/.pnpm/copy-webpack-plugin@5.1.0_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@4.46.0_webpack-cli@3.3.11/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@4.46.0_webpack-cli@3.3.11/node_modules/webpack/bin/webpack.js" "$@"
fi
