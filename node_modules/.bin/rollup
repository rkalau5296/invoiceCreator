#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/dist/bin/node_modules:/mnt/c/invoice/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/dist/node_modules:/mnt/c/invoice/node_modules/.pnpm/rollup@1.32.1/node_modules/rollup/node_modules:/mnt/c/invoice/node_modules/.pnpm/rollup@1.32.1/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/rollup/dist/bin/node_modules:/mnt/c/invoice/node_modules/rollup/dist/node_modules:/mnt/c/invoice/node_modules/rollup/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
