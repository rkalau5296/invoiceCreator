#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/@babel/parser@7.13.12/node_modules/@babel/parser/bin/node_modules:/mnt/c/invoice/node_modules/.pnpm/@babel/parser@7.13.12/node_modules/@babel/parser/node_modules:/mnt/c/invoice/node_modules/.pnpm/@babel/parser@7.13.12/node_modules/@babel/node_modules:/mnt/c/invoice/node_modules/.pnpm/@babel/parser@7.13.12/node_modules:/mnt/c/invoice/node_modules/.pnpm/@babel/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/@babel/parser/bin/node_modules:/mnt/c/invoice/node_modules/@babel/parser/node_modules:/mnt/c/invoice/node_modules/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
