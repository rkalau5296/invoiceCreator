#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/mnt/c/invoice/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/mnt/c/invoice/node_modules/.pnpm/uuid@3.4.0/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/.pnpm/webpack-log@2.0.0/node_modules/webpack-log/node_modules:/mnt/c/invoice/node_modules/.pnpm/webpack-log@2.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
else
  exec node  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
fi
