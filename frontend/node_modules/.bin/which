#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/frontend/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/which@1.3.1/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/node_modules:/mnt/c/invoice/frontend/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/frontend/node_modules/which/bin/node_modules:/mnt/c/invoice/frontend/node_modules/which/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../which/bin/which" "$@"
else
  exec node  "$basedir/../which/bin/which" "$@"
fi
