#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/json5@2.2.0/node_modules/json5/lib/node_modules:/mnt/c/invoice/node_modules/.pnpm/json5@2.2.0/node_modules/json5/node_modules:/mnt/c/invoice/node_modules/.pnpm/json5@2.2.0/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/.pnpm/loader-utils@2.0.0/node_modules/loader-utils/node_modules:/mnt/c/invoice/node_modules/.pnpm/loader-utils@2.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../json5@2.2.0/node_modules/json5/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../json5@2.2.0/node_modules/json5/lib/cli.js" "$@"
fi
