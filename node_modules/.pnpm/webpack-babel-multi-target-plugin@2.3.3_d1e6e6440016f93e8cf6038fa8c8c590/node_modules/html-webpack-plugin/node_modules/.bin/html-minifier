#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/mnt/c/invoice/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/.pnpm/html-webpack-plugin@3.2.0_webpack@4.46.0/node_modules/html-webpack-plugin/node_modules:/mnt/c/invoice/node_modules/.pnpm/html-webpack-plugin@3.2.0_webpack@4.46.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../html-minifier@3.5.21/node_modules/html-minifier/cli.js" "$@"
else
  exec node  "$basedir/../../../../../html-minifier@3.5.21/node_modules/html-minifier/cli.js" "$@"
fi
