#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/frontend/node_modules/.pnpm/uglify-js@3.4.10/node_modules/uglify-js/bin/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/uglify-js@3.4.10/node_modules/uglify-js/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/uglify-js@3.4.10/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/node_modules:/mnt/c/invoice/frontend/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/html-minifier@3.5.21/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uglify-js@3.4.10/node_modules/uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../../../../../uglify-js@3.4.10/node_modules/uglify-js/bin/uglifyjs" "$@"
fi
