#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/frontend/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/errno@0.1.8/node_modules:/mnt/c/invoice/frontend/node_modules/.pnpm/node_modules:/mnt/c/invoice/frontend/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/frontend/node_modules/errno/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../errno/cli.js" "$@"
else
  exec node  "$basedir/../errno/cli.js" "$@"
fi
