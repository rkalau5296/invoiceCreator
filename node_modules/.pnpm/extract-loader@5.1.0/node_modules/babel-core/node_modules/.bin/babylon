#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/bin/node_modules:/mnt/c/invoice/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/node_modules:/mnt/c/invoice/node_modules/.pnpm/babylon@6.18.0/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/.pnpm/babel-core@6.26.3/node_modules/babel-core/node_modules:/mnt/c/invoice/node_modules/.pnpm/babel-core@6.26.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
else
  exec node  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
fi
