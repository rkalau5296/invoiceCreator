#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/mnt/c/invoice/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/node_modules:/mnt/c/invoice/node_modules/.pnpm/import-local@2.0.0/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/import-local/fixtures/node_modules:/mnt/c/invoice/node_modules/import-local/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
