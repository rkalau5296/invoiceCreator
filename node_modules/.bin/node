#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/invoice/node_modules/.pnpm/node@14.16.0/node_modules/node/bin/node_modules:/mnt/c/invoice/node_modules/.pnpm/node@14.16.0/node_modules/node/node_modules:/mnt/c/invoice/node_modules/.pnpm/node@14.16.0/node_modules:/mnt/c/invoice/node_modules/.pnpm/node_modules:/mnt/c/invoice/node_modules:/mnt/c/node_modules:/mnt/c/invoice/node_modules/node/bin/node_modules:/mnt/c/invoice/node_modules/node/node_modules"
"$basedir/../node/bin/node.exe"   "$@"
exit $?
